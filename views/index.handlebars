<h1>BURGER WORLD </h1>

<h3>BURGER H3</h3>

<ul>
    {{#each wishes}}
    <li>
        <p>ID: {{this.id}}</p>
        <p>Wish: {{this.wish}}</p>
    </li>
    {{/each}}
</ul>

<div>
    <form action="/" method="POST">
        <textarea type="text" name="wish"></textarea>

        <button type="submit">Submit</button>
    </form>
</div>